<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_new2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".semester1"
            android:label="1학기 시간표"/>
        <activity android:name=".semester2"
            android:label="2학기 시간표"/>
        <activity android:name=".note"
            android:label="메모장"/>
        <activity android:name=".alarm"
            android:label="알람"/>
        <receiver android:name=".alramreceiver"/>
        <service
            android:name=".ringplay"
            android:enabled="true"></service>


    </application>


</manifest>